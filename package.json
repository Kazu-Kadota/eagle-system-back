  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "AWS_PROFILE=eagle-back-sdx sls offline",
    "debug": "AWS_PROFILE=eagle-back-sdx sls offline",
    "test": "LOGGER_LEVEL=silent jest",
    "lint": "tsc --noEmit && eslint --ext .ts src marreta",
    "lint-fix": "tsc --noEmit && eslint --ext .ts src --fix",
    "clean": "rimraf dist",
    "prune": "npm prune --production",
    "tsc": "tsc -p tsconfig.build.json",
    "mv:node_modules": "mv node_modules dist/node_modules",
    "cp:templates": "cp -r src/templates dist/src",
    "zip": "cd dist && zip -r latest.zip ./",
    "build": "run-s clean tsc prune mv:node_modules zip",
    "aws-lambda": "AWS_PROFILE=eagle-back AWS_REGION=us-east-1 aws lambda update-function-code --function-name teste --zip-file fileb://dist/eagle-system-back.zip"
  },
